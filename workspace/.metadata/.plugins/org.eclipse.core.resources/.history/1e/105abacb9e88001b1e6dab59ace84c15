package Q8958;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		//int testCase = sc.nextInt();
		String ox = sc.next();
		String arr[] = ox.split("");
		for(int i=0; i<arr.length; i++) System.out.println(arr[i]);
	}

}
