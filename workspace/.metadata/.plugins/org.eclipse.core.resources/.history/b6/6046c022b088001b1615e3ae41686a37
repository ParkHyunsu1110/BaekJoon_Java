package Q4344;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int testCase = sc.nextInt();
		int studentCase=0, cnt=0, sum=0;
		double aver=0, averCase=0;
		for(int i=0; i<testCase; i++) {
			studentCase = sc.nextInt();
			int studentScore[] = new int[studentCase];
			
			for(int j=0; j<studentCase; j++) studentScore[j] = sc.nextInt();
			
			for(int j=0; j<studentCase; j++) sum += studentScore[j];
			
			aver=(double)sum/studentCase;
			
			for(int j=0; j<studentCase; j++) {
				if(studentScore[j] > aver) cnt++;
			}
			averCase = (cnt / studentCase) * 100.0;
			System.out.printf("%.3f", averCase);
			System.out.println("%");
		}	
	}
}