package Q1110;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		if(N<0 || N>99) return;
		int num = N;
		int x = num/10;
		int y = num%10;
		int sum = x + y;
		N = y*10 + sum;
		System.out.println(N);
	}

}
